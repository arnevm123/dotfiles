#!/bin/bash

# $HOME/bin/vim-edit-tmux-output

# file=$(mktemp).sh
# tmux capture-pane -pS -32768 > "$file"
# tmux new-window -n mywindow "nvim '+normal G' +':%s/\\n\\s*\$//e' $file"

tmp_file=$(mktemp)
tmux capture-pane -pJS -3000 > "$tmp_file"
tmux new-window -n mywindow "nvim '-c set ft=bash' + $tmp_file"
